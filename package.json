{
  "private": true,
  "type": "module",
  "name": "react-homework",
  "version": "1.0.0",
  "description": "React Homework",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0",
    "vscode": ">=1.9.0"
  },
  "scripts": {
    "start": "pnpm dev --open",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "lint": "eslint --cache 'src/**/*.{ts,tsx}'",
    "format": "prettier --write --cache 'src/**/*.{ts,tsx}'"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@tailwindcss/vite": "4.0.3",
    "@types/node": "22.10.7",
